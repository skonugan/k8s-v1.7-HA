vrrp_instance vip {
    state MASTER
    interface {{lb_eth_name.stdout}}
    virtual_router_id 51
    priority 150
    advert_int 1
    authentication {
        auth_type PASS
        auth_pass {{keepalived_pass}}
    }
    virtual_ipaddress {
        {{k8s_lb_vip}}/{{lb_vip_prifix.stdout}} dev {{lb_eth_name.stdout}}
    }
}
